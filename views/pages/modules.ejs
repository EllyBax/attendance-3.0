<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>

  <style>
    @import url('/css/main.css');
  </style>
<script  src="/js/main.js" defer></script> </head>

<body>
    <%- include('../partials/header') %>  <main>
    <div class="navpane">
      <% if (modules.length > 0){%>
        <div class="navlinks">
        <% for (let _module of modules) { %>
            <a href="/attendance/<%= _module.code %>">
              <span style="color: green;"><%= _module.code %></span> <%= _module.name %>
            </a>
        <% } %>
      </div>
     <% } else {%>
      <h3>No modules registered here</h3>
      <% } %>
      <br>
    </div>
  </main>
    <%- include  ('../partials/footer') %>

</body>
</html>

</html>